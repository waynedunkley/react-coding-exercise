{
  "name": "react-coding-exercise",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animated": "^0.2.2",
    "body-parser": "^1.18.3",
    "chalk": "2.4.1",
    "classnames": "^2.2.6",
    "clean-css": "^4.2.1",
    "color": "^3.1.0",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "detect-it": "^3.0.5",
    "dom-helpers": "^3.4.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "escape-html": "^1.0.3",
    "express": "^4.16.4",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.15.6",
    "htmlescape": "^1.1.1",
    "identity-obj-proxy": "3.0.0",
    "lazysizes": "^4.1.5",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "memorystore": "^1.6.0",
    "morgan": "^1.9.1",
    "ms": "^2.1.1",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-app-polyfill": "^0.2.0",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^16.7.0",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^6.0.0",
    "redux-watch": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@svgr/webpack": "2.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "pnp-webpack-plugin": "1.1.0",
    "redux-mock-store": "^1.5.3",
    "resolve": "1.8.1",
    "standard": "^12.0.1",
    "standard-loader": "^6.0.1",
    "stats-webpack-plugin": "^0.7.0",
    "svg-sprite-loader": "^4.1.3",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "start:prod": "cross-env NODE_ENV=production node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "standard && node scripts/test.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupEnzyme.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/vendor"
    ]
  }
}
